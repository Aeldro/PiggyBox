@model WildPay.Models.Entities.Group
@{
    
}
<div>
    <h2>@Model.Name</h2>
    <h3>liste de dépenses</h3>
    <table>
        @foreach (var expenditure in Model.Expenditures)
        {
            <tr>
                <td>@expenditure.Name <br/>
                    payé par @expenditure.ApplicationUserId.</td> <!-- see how to get the user name -->
                <td>@expenditure.Amount</td>
            </tr>
        }
    </table>


</div>