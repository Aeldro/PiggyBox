@using Microsoft.EntityFrameworkCore.Metadata.Internal
@model List<WildPay.Models.Entities.Group>

<div>
    <h2>GROUPES</h2>
    <ul>
        @foreach (var group in Model)
        {
            <li><a asp-action="List" asp-controller="Expenditure" asp-route-id=@group.Id>@group.Name</a></li> 
        }
        <a asp-controller="Group" asp-action="Create">Nouveau groupe</a>
        <a asp-controller="Group" asp-action="Create"><span class="material-symbols-outlined">add_circle</span></a>
    </ul>
</div>