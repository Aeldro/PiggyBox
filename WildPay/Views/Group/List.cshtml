@model List<WildPay.Models.Entities.Group>

<div>
    <h2>GROUPES</h2>
        <ul>
            @foreach (var group in Model)
            {
                <li><a asp-action="Index" asp-controller="Group" asp-route-id=@group.Id>@group.Name</a></li>
            }
            <a asp-controller="Group" asp-action="Add">Nouveau groupe</a>
            <a asp-controller="Group" asp-action="Add"><span class="material-symbols-outlined">add_circle</span></a>
        </ul>
</div>